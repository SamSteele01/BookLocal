{
  "name": "booklocal",
  "homepage": "http://BookLocal.github.io/EthMemphis",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "grommet": "^1.10.1",
    "html-webpack-plugin": "2.29.0",
    "i": "^0.3.6",
    "jest": "20.0.4",
    "moment": "^2.20.1",
    "normalize.css": "^8.0.0",
    "npm": "^6.2.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "postcss-scss": "^2.0.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-datepicker": "^1.3.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-spinners": "^0.3.2",
    "style-loader": "0.19.0",
    "styled-components": "^3.1.6",
    "sw-precache-webpack-plugin": "0.11.4",
    "truffle-contract": "^3.0.3",
    "uport-connect": "^0.7.0",
    "url-loader": "0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-preset-react-app": "^3.1.2",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "gh-pages": "^1.1.0",
    "gh-pages-cli": "^0.2.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-color-function": "^4.0.1",
    "postcss-import": "^11.1.0",
    "postcss-initial": "^2.0.0",
    "postcss-math": "0.0.10",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "sass-loader": "^6.0.6"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "start": " node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "gen:sass-watch": "sass --watch src/styles/App.scss:src/styles/App.css",
    "dev": "npm-run-all --parallel start gen:sass-watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build --remote upstream"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
